{
    "name": "tictactoe-multiplayer-phaser3-colyseus",
    "version": "1.0.0",
    "scripts": {
        "start": "parcel src/client/index.html -p 8000",
        "start-server": "ts-node-dev --project tsconfig.server.json src/server/index.ts",
        "build": "parcel build src/client/index.html --out-dir dist"
    },
    "author": "mperator",
    "license": "MIT",
    "keywords": [
        "TicTacToe",
        "Multiplayer",
        "Phaser 3",
        "Colyseus",
        "TypeScript"
    ],
    "description": "Tic-Tac-Toe TypeScript multiplayer game using Phaser 3 and Colyseus.",
    "repository": {
        "type": "git",
        "url": "https://github.com/mperator/tictactoe-multiplayer-phaser3-colyseus"
    },
    "homepage": "https://github.com/mperator/tictactoe-multiplayer-phaser3-colyseus",
    "dependencies": {
        "@colyseus/command": "^0.1.6",
        "@colyseus/monitor": "^0.12.2",
        "colyseus": "^0.14.7",
        "colyseus.js": "^0.14.1",
        "cors": "^2.8.5",
        "express": "^4.16.4",
        "phaser": "^3.50.1",
        "regenerator-runtime": "^0.13.7"
    },
    "devDependencies": {
        "@types/cors": "^2.8.6",
        "@types/express": "^4.17.1",
        "minimist": ">=1.2.2",
        "parcel-bundler": "^1.12.5",
        "parcel-plugin-clean-easy": "^1.0.2",
        "parcel-plugin-static-files-copy": "^2.4.3",
        "ts-node-dev": "^1.1.1",
        "typescript": "^3.8.3"
    },
    "parcelCleanPaths": [
        "dist"
    ],
    "staticFiles": {
        "staticPath": "public",
        "watcherGlob": "**"
    }
}